{
    "name": "rectitude-open/filament-news",
    "description": "Filament news plugin with categories, tags, and revisions.",
    "keywords": [
        "rectitude-open",
        "laravel",
        "filament-news"
    ],
    "homepage": "https://github.com/rectitude-open/filament-news",
    "support": {
        "issues": "https://github.com/rectitude-open/filament-news/issues",
        "source": "https://github.com/rectitude-open/filament-news"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Aspirant Zhang",
            "email": "admin@aspirantzhang.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "codewithdennis/filament-select-tree": "^3.1",
        "cviebrock/eloquent-sluggable": "^10.0 || ^11.0 || ^12.0",
        "filament/filament": "^3.0",
        "filament/spatie-laravel-tags-plugin": "^3.3",
        "mansoor/filament-versionable": "^0.0.13",
        "ralphjsmit/laravel-filament-seo": "^1.5",
        "ralphjsmit/laravel-seo": "^1.7",
        "rectitude-open/filament-tinyeditor-6": "^6.3",
        "solution-forest/filament-tree": "^2.1",
        "spatie/laravel-package-tools": "^1.15.0",
        "tomatophp/filament-media-manager": "^1.1"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "larastan/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.1",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "RectitudeOpen\\FilamentNews\\": "src/",
            "RectitudeOpen\\FilamentNews\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RectitudeOpen\\FilamentNews\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RectitudeOpen\\FilamentNews\\FilamentNewsServiceProvider"
            ],
            "aliases": {
                "FilamentNews": "RectitudeOpen\\FilamentNews\\Facades\\FilamentNews"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
